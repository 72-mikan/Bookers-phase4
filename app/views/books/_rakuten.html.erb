<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th></th>
      <th>Title</th>
      <th>author</th>
      <th>item_caption</th>
    </tr>
  </thead>
  <tbody>
    <% books.each do |book| %>
      <tr>
        <td><%= image_tag book.medium_image_url %></td>
        <td><%= book.title %></td>
        <td><%= book.title %></td>
        <td><%= book.item_caption %></td>
      </tr>
    <% end %>
  </tbody>
</table>